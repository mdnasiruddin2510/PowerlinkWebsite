    <script type="text/javascript">
        //Load Menu
        //function loadMenu(){
            
        //}
        $.ajax({
            headers: {
                'Accept': 'application/json',
                'Content-Type': 'application/json'
            },
            url: baseURL + 'api/Home/GetAllMenu',
            type: "GET",
            async: false,
            success: function (res) {
                $("#parentMenuDP").empty();
                var parentMenuDP = ``;
                $(res).each(function (i, menu) {
                    parentMenuDP += `<li><a class="nav-link scrollto " href="${menu.url}">${menu.title}</a></li>`;
                });
                $("#parentMenuDP").append(parentMenuDP);
            }
        });
    </script>
